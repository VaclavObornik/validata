{
  "name": "validata",
  "version": "2.1.24",
  "description": "Type safe data validation and sanitization",
  "keywords": [
    "typescript",
    "type-safe",
    "typesafe",
    "typed",
    "validate",
    "validator",
    "validation",
    "clean",
    "cleanse",
    "sanitise",
    "sanitize",
    "data validator",
    "data clean",
    "data cleanse",
    "object validator",
    "request validator"
  ],
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:gregbacchus/validata.git"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/ --ext .ts,.tsx",
    "test": "./node_modules/.bin/jest --verbose --coverage --runInBand",
    "build": "rm -rf dist && tsc --outDir dist --sourceMap",
    "prepublishOnly": "npm run build && npm version patch",
    "postpublish": "git push"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "change-case": "^4.1.1",
    "eslint": "^7.12.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "jest": "^26.6.1",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.5",
    "validator": "^13.1.17"
  },
  "dependencies": {
    "@types/luxon": "^1.25.0",
    "luxon": "^1.25.0"
  }
}
